generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:root@localhost:8889/kidcity?schema=public"
}

model Card {
  id        Int    @id @default(autoincrement())
  name      String
  imageUrl  String
  
  attributes Attribute[] @relation("card_attribute")
  
}


model Attribute {
  id         Int    @id @default(autoincrement())
  trait_type String
  value String
  rarity Float 
   cards Card[] @relation("card_attribute")
  
}

